<section class="jumbotron text-center"></section>

<p id="notice"><%= notice %></p>
    <div class="album text-muted"> 
      <div class="container">
        <div class="row">
          <% @lists.each do |list| %>
              <div class="col-4">                
                  <img data-src="holder.js/100px280/thumb" alt="Card image cap">
                  <p class="card-text"><%= list.title %>  <%= list.description %> <%= list.difficulty %> </p>
                  <p class="card-text"><%= link_to 'Show', list %> | <%= link_to 'Edit', edit_list_path(list) %> | <%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %> </p>
              </div>
          <% end %>
        </div> 
      </div>
    </div>

    <footer class="text-muted">
      <div class="container">
        <p class="float-right">
          <a href="#">Back to top</a>
        </p>
        <p>All right reserve. MeLAB Global Sdn Bhd</p>
      </div>
    </footer>

